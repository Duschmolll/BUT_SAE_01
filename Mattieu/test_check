import main2
import mm


def testerVerifiaction():
    main2.secretCode=[mm.Noir,mm.Blanc,mm.Bleu,mm.Vert,mm.Vert]
    laProposition=[mm.Blanc,mm.Noir,mm.Bleu,mm.Marron,mm.Rose]
    print( main2.check(laProposition))
    
